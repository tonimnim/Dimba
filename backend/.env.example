FLASK_APP=run.py
FLASK_ENV=development

# Required in production (no defaults — app will crash if missing)
SECRET_KEY=your-secret-key-here
JWT_SECRET_KEY=your-jwt-secret-key-here

# Optional — defaults to local PostgreSQL
DATABASE_URL=postgresql://premia:premia@localhost:5432/premia

# Optional — comma-separated frontend origins
CORS_ORIGINS=http://localhost:3000
